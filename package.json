{
  "name": "bluesteel-foundry",
  "version": "1.0.0",
  "description": "React based form/page creator",
  "author": "bluesteeljk",
  "license": "MIT",
  "repository": "https://github.com/bluesteelcrm/bluesteel-foundry",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "parcel watch src/index.js",
    "build": "parcel build src/index.js --global bluesteel-founder",
