    "parcel-bundler": "^1.12.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.18.3",
    "tinymce": "^5.2.2",
    "uuid": "^8.0.0"
  },
  "files": [
    "dist"
  ]
}
